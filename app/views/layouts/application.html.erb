<!DOCTYPE html>
<html>
  <head>
    <title>ProgrammingBlog</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <%= javascript_pack_tag "application" %>
    <%= stylesheet_pack_tag "application" %>
  </head>

  <body class="blog">
    <%= render "header" %>
    <%= yield %>
    <%= render "footer" %>
  </body>
</html>
